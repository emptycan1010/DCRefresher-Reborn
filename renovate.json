{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        "group:allNonMajor",
        ":disableDependencyDashboard"
    ],
    "labels": [
        "dependencies"
    ],
    "packageRules": [
        {
            "matchCurrentVersion": "!/^0/",
            "automerge": true,
            "ignoreTests": true,
            "rangeStrategy": "bump"
        }
    ]
}
